<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dannybit.tuneflow.SearchSongActivity">
    <include android:id="@+id/toolbar_actionbar_search" layout="@layout/toolbar_default"
        android:layout_width="match_parent" android:layout_height="wrap_content" />

    <FrameLayout android:id="@+id/search_container" android:layout_width="match_parent"
        android:clickable="true" android:layout_height="match_parent"
        android:layout_below="@+id/toolbar_actionbar_search"/>

</RelativeLayout>
